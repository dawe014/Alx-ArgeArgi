<div class="user-view__form-container" id="manage">
  <h2 class="heading-secondary ma-bt-md">Manage Sell Home</h2>
  <table class="user-table">
    <thead>
      <tr>
        <th>ID number</th>
        <th>Property Name</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% homes.forEach(home => { %>
        <tr>
          <td><%= home._id %></td>
          <td><%= home.title %></td>
          <td>
            <a href="/admin/edit-home/<%= home.id %>" class="btn text-black btn--danger btn-outline-success btn--small">Edit</a>
            <form action="/admin/delete-home/<%= home._id %>" method="post" style="display:inline;">
              <button type="submit" class="btn btn--green">Delete</button>
            </form>
          </td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>